{
  "name": "@jejunetwork/dws",
  "version": "0.1.0",
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "3.954.0",
    "@aws-sdk/client-ec2": "3.954.0",
    "@elysiajs/cors": "1.4.0",
    "@elysiajs/eden": "1.4.5",
    "@jejunetwork/api": "workspace:*",
    "@jejunetwork/auth": "workspace:*",
    "@jejunetwork/config": "workspace:*",
    "@jejunetwork/contracts": "workspace:*",
    "@jejunetwork/db": "workspace:*",
    "@jejunetwork/durable-objects": "workspace:*",
    "@jejunetwork/kms": "workspace:*",
    "@jejunetwork/sdk": "workspace:*",
    "@jejunetwork/shared": "workspace:*",
    "@jejunetwork/training": "workspace:*",
    "@jejunetwork/types": "workspace:*",
    "@jejunetwork/ui": "workspace:*",
    "@noble/curves": "1.9.7",
    "@noble/hashes": "2.0.1",
    "@rainbow-me/rainbowkit": "2.2.9",
    "@solana/buffer-layout": "4.0.1",
    "@solana/web3.js": "1.98.0",
    "@tanstack/react-query": "5.79.0",
    "@types/sharp": "0.32.0",
    "bigint-buffer": "1.1.5",
    "cockatiel": "3.2.1",
    "croner": "9.0.0",
    "elysia": "1.4.19",
    "lru-cache": "11.2.4",
    "lucide-react": "0.513.0",
    "mime": "3.0.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router-dom": "7.6.1",
    "sharp": "0.34.5",
    "viem": "2.43.3",
    "wagmi": "2.17.4",
    "webtorrent": "2.8.5",
    "yaml": "2.8.2",
    "zod": "4.2.1"
  },
  "devDependencies": {
    "@jejunetwork/tests": "workspace:*",
    "@playwright/test": "^1.57.0",
    "@types/bun": "^1.3.5",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@upstash/redis": "^1.36.0",
    "ioredis": "^5.8.2",
    "redis": "^5.10.0",
    "typescript": "5.9.3"
  },
  "description": "Decentralized Web Services - storage, compute, CDN, training, and infrastructure",
  "type": "module",
  "main": "api/index.ts",
  "exports": {
    ".": "./api/index.ts",
    "./api/utils/event-emitter": "./api/utils/event-emitter.ts"
  },
  "scripts": {
    "dev": "bun run scripts/dev-with-infra.ts",
    "dev:simple": "concurrently \"bun run scripts/dev-frontend.ts\" \"bun run api/server/index.ts\"",
    "dev:api": "NETWORK=localnet PORT=4030 bun run api/server/index.ts",
    "dev:frontend": "PORT=4031 bun run scripts/dev-frontend.ts",
    "deploy:contracts": "bun run scripts/deploy-contracts.ts",
    "build": "bun build api/server/index.ts --outdir dist --target bun --minify && bun build.web.ts",
    "build:web": "bun build.web.ts",
    "start": "bun run api/server/index.ts",
    "serve": "bun run scripts/serve.ts",
    "deploy": "bun run scripts/deploy.ts",
    "test": "bun test tests/*.test.ts tests/sdk-compatibility/*.test.ts tests/mcp-a2a/*.test.ts",
    "typecheck": "tsc --noEmit",
    "install:workerd": "bun run scripts/install-workerd.ts"
  }
}
