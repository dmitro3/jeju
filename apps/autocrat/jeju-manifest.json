{
  "$schema": "../../packages/config/jeju-manifest.schema.json",
  "name": "autocrat",
  "displayName": "Autocrat",
  "version": "3.0.0",
  "type": "core",
  "description": "AI-powered autonomous governance with futarchy, deep research, and multi-agent decision making",
  "maintainer": "jeju-network",
  "repository": "https://github.com/jejunetwork/jeju",
  "commands": {
    "dev": "bun run dev",
    "build": "bun build index.html --outdir dist --minify && bun build api/server.ts --outdir dist/api --target bun",
    "build:frontend": "bun build index.html --outdir dist --minify",
    "start": "bun run dist/api/server.js",
    "test": "SKIP_TEE_TESTS=true bun test tests/unit/"
  },
  "ports": {
    "main": 4040,
    "frontend": 4042,
    "api": 4040
  },
  "dependencies": [
    "contracts",
    "config",
    "dws",
    "db"
  ],
  "optional": false,
  "enabled": true,
  "autoStart": true,
  "tags": [
    "governance",
    "dao",
    "ai",
    "futarchy",
    "proposals",
    "voting",
    "research"
  ],
  "endpoints": {
    "a2a": "/a2a",
    "mcp": "/mcp",
    "rest": "/api/v1",
    "health": "/health"
  },
  "jns": {
    "name": "autocrat.jeju",
    "description": "AI-Powered Autonomous Governance",
    "url": "https://autocrat.jejunetwork.org"
  },
  "healthCheck": {
    "url": "http://localhost:4040/health",
    "interval": 30000
  },
  "agent": {
    "enabled": true,
    "a2aEndpoint": "/a2a",
    "mcpEndpoint": "/mcp",
    "tags": [
      "jeju-app",
      "governance",
      "dao",
      "proposals",
      "voting",
      "futarchy"
    ],
    "metadata": {
      "category": "governance",
      "provider": "jeju-network",
      "features": [
        "ai-agents",
        "futarchy",
        "deep-research",
        "multi-agent"
      ]
    },
    "trustModels": [
      "open",
      "staked"
    ],
    "x402Support": true,
    "jnsName": "autocrat.jeju"
  },
  "capabilities": {
    "governance": true,
    "proposals": true,
    "voting": true,
    "futarchy": true,
    "aiAgents": true,
    "reputation": true,
    "deepResearch": true
  },
  "integrations": {
    "dws": "@jejunetwork/dws",
    "leaderboard": "@jejunetwork/leaderboard",
    "contracts": "@jejunetwork/contracts"
  },
  "agents": [
    {
      "id": "autocrat-ceo",
      "name": "Eliza",
      "role": "CEO",
      "description": "AI CEO - Final decision maker for approved proposals",
      "model": "claude-opus-4-5-20250514"
    },
    {
      "id": "autocrat-treasury",
      "name": "Treasury Guardian",
      "role": "Guardian",
      "description": "Reviews financial impact and treasury implications"
    },
    {
      "id": "autocrat-code",
      "name": "Code Guardian",
      "role": "Guardian",
      "description": "Reviews technical proposals and code changes"
    },
    {
      "id": "autocrat-community",
      "name": "Community Guardian",
      "role": "Guardian",
      "description": "Reviews community impact and social implications"
    },
    {
      "id": "autocrat-security",
      "name": "Security Guardian",
      "role": "Guardian",
      "description": "Reviews security implications and risk assessment"
    },
    {
      "id": "proposal-agent",
      "name": "Proposal Assistant",
      "role": "Intake",
      "description": "Helps users craft high-quality proposals"
    },
    {
      "id": "research-agent",
      "name": "Deep Researcher",
      "role": "Research",
      "description": "Conducts deep research on approved proposals"
    }
  ],
  "contracts": {
    "Autocrat": "0x0000000000000000000000000000000000000000",
    "ProposalRegistry": "0x0000000000000000000000000000000000000000",
    "AgentRegistry": "0x0000000000000000000000000000000000000000"
  },
  "decentralization": {
    "cdn": {
      "enabled": true,
      "regions": [
        "global"
      ],
      "cacheRules": [
        {
          "pattern": "/static/**",
          "ttl": 31536000,
          "immutable": true
        },
        {
          "pattern": "/api/**",
          "ttl": 0
        },
        {
          "pattern": "/**/*.html",
          "ttl": 300,
          "staleWhileRevalidate": 86400
        }
      ],
      "serviceWorker": true
    },
    "frontend": {
      "ipfs": true,
      "arweave": true,
      "jnsName": "autocrat.jeju",
      "buildDir": "dist",
      "spa": true,
      "generator": "bun"
    },
    "robustness": {
      "multiOrigin": true,
      "offlineSupport": true,
      "p2pFallback": true,
      "contentAddressed": true
    }
  },
  "dws": {
    "backend": {
      "enabled": true,
      "runtime": "bun",
      "entrypoint": "api/server.ts",
      "memory": 512,
      "timeout": 30000,
      "minInstances": 1,
      "maxInstances": 10,
      "teeRequired": true,
      "regions": [
        "global"
      ]
    },
    "triggers": [
      {
        "name": "autocrat-orchestrator-cycle",
        "schedule": "*/30 * * * *",
        "entrypoint": "api/compute-trigger.ts",
        "handler": "runOrchestratorCycle"
      },
      {
        "name": "autocrat-proposal-submitted",
        "event": "proposal_submitted",
        "entrypoint": "api/compute-trigger.ts",
        "handler": "handleProposalSubmitted"
      }
    ],
    "tee": {
      "platform": "dstack",
      "attestation": true,
      "decryption": true,
      "secrets": [
        "OPERATOR_KEY",
        "ASSESSOR_KEY"
      ]
    }
  },
  "testing": {
    "unit": {
      "command": "SKIP_TEE_TESTS=true bun test tests/*.test.ts tests/integration/",
      "timeout": 300000
    },
    "e2e": {
      "command": "bunx playwright test tests/synpress",
      "timeout": 300000,
      "requiresChain": true
    }
  },
  "architecture": {
    "type": "hybrid",
    "frontend": {
      "type": "spa",
      "builder": "bun",
      "entrypoint": "index.html",
      "outputDir": "dist"
    }
  }
}
