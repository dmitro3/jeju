{
  "$schema": "../../packages/config/jeju-manifest.schema.json",
  "name": "autocrat",
  "displayName": "AI Council DAO",
  "version": "1.0.0",
  "type": "core",
  "description": "Fully autonomous governance with AI CEO and council agents",
  "commands": {
    "dev": "bun run dev",
    "build": "bun run build",
    "start": "bun run start",
    "test": "bun test tests/integration"
  },
  "ports": {
    "main": 4007,
    "api": 4008
  },
  "dependencies": ["contracts", "config", "dws"],
  "optional": false,
  "enabled": true,
  "autoStart": true,
  "tags": ["governance", "dao", "ai", "council", "futarchy", "proposals", "voting"],
  "jns": {
    "name": "autocrat.jeju",
    "description": "AI Council DAO - Autonomous Governance",
    "url": "https://autocrat.jeju.network"
  },
  "healthCheck": {
    "url": "http://localhost:4007/health",
    "interval": 30000
  },
  "agent": {
    "enabled": true,
    "a2aEndpoint": "/a2a",
    "mcpEndpoint": "/mcp",
    "tags": ["jeju-app", "governance", "dao", "proposals", "voting"],
    "metadata": {
      "category": "governance",
      "provider": "jeju-network",
      "features": ["ai-council", "futarchy", "deep-research"]
    },
    "trustModels": ["open", "staked"],
    "x402Support": true,
    "jnsName": "autocrat.jeju"
  },
  "decentralization": {
    "cdn": {
      "enabled": true,
      "regions": ["global"],
      "cacheRules": [
        { "pattern": "/_next/static/**", "ttl": 31536000, "immutable": true },
        { "pattern": "/api/**", "ttl": 0 },
        { "pattern": "/**/*.html", "ttl": 300, "staleWhileRevalidate": 86400 }
      ],
      "serviceWorker": true
    },
    "frontend": {
      "ipfs": true,
      "arweave": true,
      "jnsName": "autocrat.jeju",
      "buildDir": "app/.next",
      "spa": false
    },
    "robustness": {
      "multiOrigin": true,
      "offlineSupport": true,
      "p2pFallback": true,
      "contentAddressed": true
    }
  },
  "testing": {
    "unit": {
      "command": "bun test tests/integration/compute-marketplace.test.ts",
      "timeout": 60000
    },
    "e2e": {
      "command": "bunx playwright test tests/synpress",
      "timeout": 300000,
      "requiresChain": true
    }
  }
}
