{
  "name": "@jeju/autocrat",
  "version": "1.0.0",
  "description": "AI Council DAO - Autonomous governance with AI CEO, council agents, and reputation-weighted proposals",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun run src/index.ts",
    "dev:ceo": "bun run src/ceo-server.ts",
    "dev:all": "bun run src/index.ts & bun run src/ceo-server.ts",
    "build": "bun build src/index.ts --outdir dist --target node",
    "test": "bun test tests/integration/",
    "test:api": "npx playwright test tests/synpress/",
    "test:e2e": "npx playwright test app/e2e-tests/",
    "test:all": "bun test tests/integration/ && npx playwright test tests/synpress/",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@elizaos/core": "1.6.5",
    "@anthropic-ai/sdk": "0.30.1",
    "@jejunetwork/config": "workspace:*",
    "@jejunetwork/db": "workspace:*",
    "@jejunetwork/shared": "workspace:*",
    "@jejunetwork/sdk": "workspace:*",
    "@jejunetwork/eliza-plugin": "workspace:*",
    "ai": "5.0.52",
    "hono": "4.7.0",
    "viem": "2.41.2",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@playwright/test": "1.57.0",
    "@synthetixio/synpress": "4.1.1",
    "@types/bun": "latest",
    "typescript": "5.7.3"
  }
}
