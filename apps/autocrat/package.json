    "ai": "^5.0.52",
    "hono": "4.11.1",
    "viem": "2.43.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@jejunetwork/tests": "workspace:*",
    "@playwright/test": "1.57.0",
