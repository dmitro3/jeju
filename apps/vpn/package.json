{
  "name": "@jejunetwork/vpn",
  "version": "0.1.0",
  "dependencies": {
    "@elysiajs/cors": "^1.3.1",
    "@jejunetwork/config": "workspace:*",
    "@jejunetwork/sdk": "workspace:*",
    "@jejunetwork/types": "workspace:*",
    "elysia": "^1.4.19",
    "lucide-react": "0.562.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "viem": "2.43.3",
    "zod": "4.2.1"
  },
  "devDependencies": {
    "@jejunetwork/tests": "workspace:*",
    "@playwright/test": "1.57.0",
    "@synthetixio/synpress": "4.1.1",
    "@tauri-apps/api": "2.0.0",
    "@tauri-apps/cli": "2.0.0",
    "@types/bun": "latest",
    "@types/react": "19.2.3",
    "@types/react-dom": "^19.0.0",
    "concurrently": "9.1.0",
    "playwright-core": "1.57.0",
    "tailwindcss": "3.4.17",
    "typescript": "5.9.3"
  },
  "description": "Jeju VPN - Free, decentralized VPN powered by the community",
  "type": "module",
  "main": "api/index.ts",
  "scripts": {
    "dev": "bun run scripts/dev.ts",
    "build": "bun run scripts/build.ts",
    "start": "bun run scripts/serve.ts",
    "deploy": "bun run scripts/deploy.ts",
    "test": "bun test lib/ api/",
    "test:e2e": "bunx playwright test tests/e2e/",
    "test:synpress": "bunx synpress run --config synpress.config.ts tests/synpress/",
    "typecheck": "tsc --noEmit",
    "dev:app": "cd app/src-tauri && tauri dev",
    "build:app": "cd app/src-tauri && tauri build",
    "build:extension": "bun extension/scripts/generate-icons.ts && zip -r dist/vpn-extension.zip extension -x 'extension/node_modules/*' -x 'extension/scripts/*' -x 'extension/package.json'"
  }
}
