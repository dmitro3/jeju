{
  "name": "@jejunetwork/otto",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Otto - Decentralized multi-platform AI trading agent for Discord, Telegram, Twitter, and WhatsApp",
  "main": "api/index.ts",
  "scripts": {
    "dev": "bun run api/server.ts",
    "build": "bun build api/server.ts --outdir dist --target bun --entry-naming server.js && bun build api/index.ts --outdir dist --target bun",
    "start": "bun dist/server.js",
    "test": "bun test tests/unit/",
    "test:e2e": "bunx playwright test --config=playwright.config.ts",
    "test:synpress": "bunx synpress --config=synpress.config.ts",
    "test:all": "bun run test && bun run test:e2e",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@elizaos/core": "^1.0.0",
    "@elizaos/adapter-sqlite": "0.25.6-alpha.1",
    "@elizaos/plugin-twitter": "^1.0.0",
    "@elizaos/plugin-discord": "^1.0.0",
    "@elizaos/plugin-telegram": "^1.0.0",
    "@elysiajs/cors": "^1.3.1",
    "@jejunetwork/config": "workspace:*",
    "@jejunetwork/db": "workspace:*",
    "@jejunetwork/shared": "workspace:*",
    "@jejunetwork/types": "workspace:*",
    "@jejunetwork/sdk": "workspace:*",
    "@jejunetwork/kms": "workspace:*",
    "@jejunetwork/oauth3": "workspace:*",
    "@solana/web3.js": "1.98.4",
    "discord.js": "14.16.3",
    "elysia": "^1.3.1",
    "telegraf": "4.16.3",
    "twilio": "5.4.0",
    "viem": "2.43.3",
    "zod": "4.2.1"
  },
  "devDependencies": {
    "@jejunetwork/tests": "workspace:*",
    "@playwright/test": "1.57.0",
    "@synthetixio/synpress": "4.1.1",
    "@types/bun": "latest",
    "typescript": "5.9.3"
  }
}
