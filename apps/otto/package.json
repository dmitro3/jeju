    "@elizaos/plugin-discord": "^1.0.0",
    "@elizaos/plugin-telegram": "^1.0.0",
    "@elysiajs/cors": "1.4.1",
    "@jejunetwork/auth": "workspace:*",
    "@jejunetwork/cache": "workspace:*",
    "@jejunetwork/config": "workspace:*",
    "@jejunetwork/db": "workspace:*",
    "@jejunetwork/shared": "workspace:*",
    "@jejunetwork/sqlit": "workspace:*",
    "@jejunetwork/types": "workspace:*",
    "@jejunetwork/sdk": "workspace:*",
    "@jejunetwork/kms": "workspace:*",
    "@jejunetwork/oauth3": "workspace:*",
    "@solana/web3.js": "1.98.4",
    "discord.js": "14.16.3",
    "elysia": "1.4.21",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "telegraf": "4.16.3",
    "twilio": "5.4.0",
    "viem": "2.43.3",
    "zod": "4.2.1"
  },
  "devDependencies": {
    "@jejunetwork/tests": "workspace:*",
    "@playwright/test": "1.57.0",
    "@synthetixio/synpress": "4.1.1",
    "@types/bun": "latest",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "typescript": "5.9.3"
  },
  "private": true,
  "type": "module",
  "description": "Otto - Decentralized multi-platform AI trading agent for Discord, Telegram, Twitter, and WhatsApp",
  "main": "api/index.ts",
  "scripts": {
    "dev": "bun run scripts/dev.ts",
    "dev:api": "bun --watch api/server.ts",
    "dev:worker": "bun --watch api/worker.ts",
    "build": "bun run scripts/build.ts",
    "start": "bun run scripts/serve.ts",
    "deploy": "bun run scripts/deploy.ts",
    "deploy:testnet": "NETWORK=testnet bun run scripts/deploy.ts",
    "deploy:mainnet": "NETWORK=mainnet bun run scripts/deploy.ts",
    "test": "bun test tests/unit/",
    "test:e2e": "bunx playwright test --config=playwright.config.ts",
    "test:synpress": "bunx synpress --config=synpress.config.ts",
    "typecheck": "tsc --noEmit"
  }
}
