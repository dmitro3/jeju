{
  "name": "@jejunetwork/documentation",
  "version": "1.0.0",
  "description": "Network documentation site powered by Vocs",
  "type": "module",
  "scripts": {
    "dev": "vocs dev --port ${DOCUMENTATION_PORT:-4004}",
    "a2a": "bun run server/a2a-server.ts",
    "build": "vocs build",
    "preview": "vocs preview --port ${DOCUMENTATION_PORT:-4004}",
    "test": "bun run test:unit",
    "test:full": "bun run test:e2e && bun run test:unit",
    "test:unit": "bun test tests/unit/",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed"
  },
  "dependencies": {
    "@jejunetwork/config": "workspace:*",
    "cors": "2.8.5",
    "express": "5.0.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "remark-stringify": "11.0.0",
    "unified": "11.0.5",
    "vocs": "1.4.0",
    "zod": "3.24.4"
  },
  "devDependencies": {
    "@jejunetwork/tests": "workspace:*",
    "@playwright/test": "1.57.0",
    "@types/cors": "2.8.17",
    "@types/express": "5.0.3",
    "@types/react": "18.3.23",
    "@types/react-dom": "18.3.7"
  }
}
