{
  "name": "@jejunetwork/documentation",
  "version": "0.1.0",
  "description": "Network documentation site powered by Vocs",
  "type": "module",
  "scripts": {
    "dev": "bun run scripts/dev.ts",
    "build": "bun run scripts/build.ts",
    "start": "bun run scripts/serve.ts",
    "deploy": "bun run scripts/deploy.ts",
"test": "bun test tests/unit/",
    "typecheck": "tsc --noEmit",
    "dev:vocs": "vocs dev --port ${PORT:-4055}",
    "a2a": "bun run server/a2a-server.ts",
    "build:docs-index": "bun run scripts/build-docs-index.ts"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.0",
    "@jejunetwork/config": "workspace:*",
    "@jejunetwork/shared": "workspace:*",
    "@jejunetwork/types": "workspace:*",
    "elysia": "^1.4.19",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "vocs": "1.4.0",
    "zod": "4.2.1"
  },
  "devDependencies": {
    "@jejunetwork/tests": "workspace:*",
    "@playwright/test": "1.57.0",
    "@types/react": "18.3.23",
    "@types/react-dom": "18.3.7"
  }
}
