    "@jejunetwork/auth": "workspace:*",
    "@jejunetwork/cache": "workspace:*",
    "@jejunetwork/config": "workspace:*",
    "@jejunetwork/contracts": "workspace:*",
    "@jejunetwork/db": "workspace:*",
    "@jejunetwork/messaging": "workspace:*",
    "@jejunetwork/sdk": "workspace:*",
    "@jejunetwork/shared": "workspace:*",
    "@jejunetwork/types": "workspace:*",
    "@jejunetwork/ui": "workspace:*",
    "@tanstack/react-query": "5.90.12",
    "bs58": "^6.0.0",
    "clsx": "2.1.1",
    "elysia": "^1.4.19",
    "lucide-react": "0.562.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-router-dom": "7.6.1",
    "sonner": "1.7.2",
    "viem": "2.43.3",
    "wagmi": "2.18.1",
    "zod": "4.2.1"
  },
  "devDependencies": {
    "@jejunetwork/tests": "workspace:*",
    "@playwright/test": "1.57.0",
    "@synthetixio/synpress": "4.1.1",
    "@types/bun": "1.2.16",
    "@types/node": "22.15.30",
    "@types/react": "19.2.3",
    "@types/react-dom": "19.2.3",
    "autoprefixer": "10.4.21",
    "bun-plugin-tailwind": "^0.1.2",
    "concurrently": "^9.2.1",
    "playwright-core": "1.57.0",
    "postcss": "8.5.3",
    "tailwindcss": "3.4.17",
    "typescript": "5.9.3"
  },
  "description": "Developer coordination hub - bounties, jobs, git, packages, containers, models, project management",
  "private": true,
  "type": "module",
  "main": "api/server.ts",
  "scripts": {
    "dev": "concurrently \"bun run scripts/dev.ts\" \"API_PORT_OFFSET=1 bun --watch api/server.ts\"",
    "build": "bun run scripts/build.ts",
    "start": "bun run scripts/serve.ts",
    "test": "bun test tests/unit/",
    "test:e2e": "bunx playwright test tests/e2e/",
    "test:synpress": "bunx playwright test --config synpress.config.ts tests/synpress/",
    "deploy": "bun run scripts/deploy.ts",
    "deploy:testnet": "NETWORK=testnet DWS_URL=https://dws.testnet.jejunetwork.org bun run scripts/deploy.ts",
    "typecheck": "tsc --noEmit"
  }
}
