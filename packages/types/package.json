{
  "name": "@jejunetwork/types",
  "version": "0.1.0",
  "description": "TypeScript types for Jeju Network",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./indexer": {
      "import": "./dist/indexer.js",
      "types": "./dist/indexer.d.ts"
    },
    "./names": {
      "import": "./dist/names.js",
      "types": "./dist/names.d.ts"
    },
    "./rpc": {
      "import": "./dist/rpc.js",
      "types": "./dist/rpc.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elizaos/jeju.git",
    "directory": "packages/types"
  },
  "keywords": [
    "jeju",
    "types",
    "typescript",
    "ethereum",
    "oif",
    "eil"
  ],
  "devDependencies": {
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "zod": ">=4.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
