{
  "name": "@jejunetwork/cli",
  "version": "0.1.0",
  "description": "Network CLI - Development, testing, deployment, and key management",
  "license": "MIT",
  "type": "module",
  "bin": {
    "jeju": "./bin/jeju.js",
    "git-remote-jeju": "./bin/git-remote-jeju.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist", "bin", "templates"],
  "scripts": {
    "build": "bun build src/index.ts src/commands/git-remote.ts --outdir dist --target bun --external electron --external playwright-core",
    "dev": "bun run src/index.ts",
    "test": "bun test",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@elysiajs/cors": "1.3.0",
    "@elysiajs/eden": "^1.4.5",
    "@jejunetwork/config": "workspace:*",
    "@jejunetwork/shared": "workspace:*",
    "@jejunetwork/types": "workspace:*",
    "@noble/curves": "2.0.1",
    "@noble/hashes": "1.7.1",
    "chalk": "5.4.1",
    "commander": "12.1.0",
    "elysia": "^1.4.19",
    "execa": "9.5.3",
    "prompts": "2.4.2",
    "viem": "2.43.3",
    "which": "4.0.0"
  },
  "optionalDependencies": {
    "@phala/dstack-sdk": "0.5.7"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/prompts": "2.4.9",
    "@types/which": "3.0.4",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "@jejunetwork/config": ">=0.1.0",
    "@jejunetwork/contracts": ">=0.1.0",
    "@jejunetwork/types": ">=0.1.0",
    "@jejunetwork/tests": ">=0.1.0",
    "@playwright/test": ">=1.55.0",
    "@synthetixio/synpress": ">=4.0.0",
    "zod": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "@jejunetwork/config": { "optional": true },
    "@jejunetwork/contracts": { "optional": true },
    "@jejunetwork/types": { "optional": true },
    "@jejunetwork/tests": { "optional": true },
    "@playwright/test": { "optional": true },
    "@synthetixio/synpress": { "optional": true },
    "zod": { "optional": true }
  },
  "publishConfig": {
    "access": "public"
  }
}
