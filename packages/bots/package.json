{
  "name": "@jejunetwork/bots",
  "version": "1.0.0",
  "type": "module",
  "description": "MEV, arbitrage, liquidity management, and TFMM bots for Jeju Network",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./strategies": {
      "import": "./src/strategies/index.ts",
      "types": "./src/strategies/index.ts"
    },
    "./engine": {
      "import": "./src/engine/index.ts",
      "types": "./src/engine/index.ts"
    },
    "./simulation": {
      "import": "./src/simulation/index.ts",
      "types": "./src/simulation/index.ts"
    }
  },
  "scripts": {
    "dev": "bun run src/cli.ts",
    "test": "bun test src/",
    "test:watch": "bun test --watch src/",
    "test:simulation": "bun test src/simulation/",
    "backtest": "bun run src/cli.ts backtest",
    "simulate": "bun run src/cli.ts simulate",
    "analyze": "bun run src/analyze.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@jejunetwork/config": "workspace:*",
    "@jejunetwork/contracts": "workspace:*",
    "@jejunetwork/types": "workspace:*",
    "@solana/web3.js": "^1.98.0",
    "viem": "2.41.2"
  },
  "devDependencies": {
    "@types/bun": "1.3.4",
    "typescript": "5.9.3"
  }
}

