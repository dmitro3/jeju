{
  "name": "@jejunetwork/sqlit",
  "version": "0.1.0",
  "dependencies": {
    "@elysiajs/cors": "1.4.1",
    "@jejunetwork/types": "workspace:*",
    "elysia": "1.4.21",
    "viem": "2.43.3"
  },
  "devDependencies": {
    "@types/bun": "1.3.5",
    "@types/node": "^20.0.0",
    "typescript": "5.9.3"
  },
  "type": "module",
  "description": "SQLit - Permissionless distributed SQL database for Jeju Network",
  "keywords": [
    "SQLit",
    "database",
    "decentralized",
    "SQL",
    "blockchain",
    "jeju",
    "typescript",
    "distributed",
    "permissionless"
  ],
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./client": {
      "bun": "./src/client.ts",
      "import": "./src/client.ts",
      "types": "./src/client.ts"
    },
    "./server": {
      "bun": "./src/server.ts",
      "import": "./src/server.ts",
      "types": "./src/server.ts"
    },
    "./node": {
      "bun": "./src/node.ts",
      "import": "./src/node.ts",
      "types": "./src/node.ts"
    },
    "./tee": {
      "bun": "./src/tee.ts",
      "import": "./src/tee.ts",
      "types": "./src/tee.ts"
    },
    "./types": {
      "bun": "./src/types.ts",
      "import": "./src/types.ts",
      "types": "./src/types.ts"
    }
  },
  "files": [
    "src",
    "README.md"
  ],
  "scripts": {
    "dev": "bun run src/server.ts",
    "test": "bun test src/",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/jejunetwork/jeju",
    "directory": "packages/sqlit"
  }
}
