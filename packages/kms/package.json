{
  "name": "@jejunetwork/kms",
  "version": "0.2.0",
  "description": "Decentralized Key Management System for Jeju Network - MPC/TEE based, no external dependencies",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./sdk": {
      "types": "./dist/sdk/index.d.ts",
      "import": "./dist/sdk/index.js"
    },
    "./mpc": {
      "types": "./dist/mpc/index.d.ts",
      "import": "./dist/mpc/index.js"
    },
    "./vault": {
      "types": "./dist/vault/index.d.ts",
      "import": "./dist/vault/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "bun test",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elizaos/jeju.git",
    "directory": "packages/kms"
  },
  "keywords": [
    "jeju",
    "kms",
    "mpc",
    "tee",
    "threshold-signatures",
    "key-management",
    "shamir",
    "cryptography"
  ],
  "dependencies": {
    "viem": "2.41.2",
    "@jejunetwork/oauth3": "workspace:*"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "5.7.3"
  },
  "peerDependencies": {
    "viem": ">=2.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
