{
  "name": "@jejunetwork/api",
  "version": "0.1.0",
  "description": "Framework-agnostic API middleware with auth, rate limiting, and error handling",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elizaos/jeju.git",
    "directory": "packages/api"
  },
  "keywords": [
    "jeju",
    "api",
    "middleware",
    "auth",
    "rate-limiting",
    "elysia"
  ],
  "dependencies": {
    "@jejunetwork/types": "workspace:*",
    "viem": "2.43.3",
    "zod": "4.2.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "elysia": "^1.0.0",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "elysia": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "elysia": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
