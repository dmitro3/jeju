  "scripts": {
    "preinstall": "python3 -c 'import distutils' 2>/dev/null || (echo 'Python distutils not found - run: pip3 install --user --break-system-packages setuptools' && exit 1)",
    "postinstall": "bun run build:packages && bun run packages/deployment/scripts/setup-apps.ts || echo 'Setup completed with warnings'",
    "build:packages": "cd packages/types && bun run build && cd ../contracts && bun run build",
    "jeju": "bun run packages/cli/src/index.ts",
    "dev": "bun run jeju -- dev",
    "start": "bun run jeju -- dev",
    "build": "bun run jeju -- build",
    "clean": "bun run jeju -- clean",
    "status": "bun run jeju -- status",
    "keys": "bun run jeju -- keys",
    "lint": "biome check --write packages apps && ruff check packages apps vendor/babylon --fix && ruff format packages apps vendor/babylon && cd apps/node/src-tauri && cargo fmt && cd ../../vpn/src-tauri && cargo fmt && cd ../../wallet/src-tauri && cargo fmt",
    "lint:check": "biome check packages apps && ruff check packages apps vendor/babylon && cd apps/node/src-tauri && cargo fmt --check && cd ../../vpn/src-tauri && cargo fmt --check && cd ../../wallet/src-tauri && cargo fmt --check",
    "lint:clippy": "cd apps/node/src-tauri && cargo clippy --all-targets -- -W clippy::all; cd ../../vpn/src-tauri && cargo clippy --all-targets -- -W clippy::all; cd ../../wallet/src-tauri && cargo clippy --all-targets -- -W clippy::all; cd ../../../packages/solana && cargo clippy --all-targets -- -W clippy::all; cd ../bridge/circuits && cargo clippy --all-targets -- -W clippy::all",
    "typecheck": "turbo run typecheck --filter='!./vendor/*' --filter='!./vendor/*/packages/*' --filter='!./vendor/*/apps/*' && tsc --noEmit && python3 -m pyright packages/training/python apps/dws/src vendor/babylon/packages/training/python vendor/babylon/packages/examples/babylon-langgraph-agent && cd apps/node/src-tauri && cargo check && cd ../../vpn/src-tauri && cargo check && cd ../../wallet/src-tauri && cargo check",
    "test": "bun run jeju -- test",
    "test:unit": "bun run jeju -- test --phase=unit",
    "test:contracts": "bun run jeju -- test --phase=contracts",
    "test:e2e": "bun run jeju -- test --mode=e2e",
    "test:load": "cd packages/tests && bun run load:all",
    "forge": "cd packages/contracts && forge",
    "forge:test": "cd packages/contracts && forge test",
    "forge:build": "cd packages/contracts && forge build",
    "localnet": "cd packages/deployment && bun run localnet:start",
    "localnet:stop": "cd packages/deployment && bun run localnet:stop",
    "localnet:reset": "cd packages/deployment && bun run localnet:reset",
    "deploy": "bun run jeju -- deploy",
    "deploy:testnet": "bun run jeju -- deploy testnet-full",
    "deploy:mainnet": "bun run jeju -- deploy mainnet",
    "deploy:app": "bun run jeju -- deploy app",
    "deploy:oif": "bun run packages/deployment/scripts/deploy/oif.ts",
    "infra:plan": "cd packages/deployment && bun run infra:plan",
    "infra:apply": "cd packages/deployment && bun run infra:apply",
    "deps:check": "bunx madge --circular --extensions ts packages/ --exclude 'lib/account-abstraction' && bunx depcheck --ignores='@playwright/test,@synthetixio/synpress,knip,madge,depcheck,playwright,playwright-core,prettier,prettier-plugin-solidity,solhint,turbo,typedoc-plugin-markdown,bun,bun:test,@jejunetwork/*,@babylon/*,@aws-sdk/*,@google-cloud/*,prom-client,@eth-optimism/*,@noble/*,@opentelemetry/*,@openzeppelin/*,@safe-global/*,@synthetixio/*,@uniswap/*,ioredis,lru-cache,pg,tar,@types/pg,@types/ws,vitepress,ws,yaml-lint' --ignore-patterns='vendor/**,8004/**,**/node_modules/**,packages/contracts/lib/**'",
    "ci": "bun run lint:check && bun run typecheck && bun run deps:check && bun run test:unit",

    "changeset": "changeset",
    "version": "changeset version",
    "publish:check": "turbo run build --filter='./packages/*' && bun run scripts/publish-check.ts",
    "publish:packages": "turbo run build --filter='./packages/*' && changeset publish"
  },
