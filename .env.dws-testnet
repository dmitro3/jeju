# DWS Testnet Configuration
# Pure decentralized deployment configuration

# Network
NETWORK=testnet
NODE_ENV=production

# Deployer
DEPLOYER_PRIVATE_KEY=${DEPLOYER_PRIVATE_KEY}

# DWS Endpoints
DWS_URL=https://dws.testnet.jejunetwork.org
IPFS_GATEWAY_URL=https://ipfs.testnet.jejunetwork.org
JNS_GATEWAY_URL=https://jns.testnet.jejunetwork.org

# TEE Configuration - Phala
PHALA_API_KEY=phak_ycVEhuwQsLmTzQRaFVkTeWAx9Sk5qWujbU2H4Ki4Mh4
PHALA_ENDPOINT=https://api.phala.network
TEE_MODE=phala
TEE_PLATFORM=phala
TEE_REQUIRED=false

# Contract Addresses (will be populated from deployment)
JNS_REGISTRY_ADDRESS=${JNS_REGISTRY_ADDRESS}
JNS_RESOLVER_ADDRESS=${JNS_RESOLVER_ADDRESS}
JNS_REGISTRAR_ADDRESS=${JNS_REGISTRAR_ADDRESS}

# RPC
TESTNET_RPC_URL=https://sepolia.base.org
L1_RPC_URL=https://ethereum-sepolia-rpc.publicnode.com

# Storage
STORAGE_BACKEND=ipfs
IPFS_PIN_TO_DWS=true

# DNS Patterns (both supported)
# - *.testnet.jejunetwork.org (primary, less ugly)
# - *.jns.testnet.jejunetwork.org (explicit JNS)
DNS_PRIMARY_PATTERN=testnet.jejunetwork.org
DNS_JNS_PATTERN=jns.testnet.jejunetwork.org

# Ingress
INGRESS_ENABLED=true
INGRESS_TLS_AUTO=true

# Decentralization Settings
APIS_DECENTRALIZED=true
FRONTEND_ON_IPFS=true
BACKEND_ON_DWS=true
